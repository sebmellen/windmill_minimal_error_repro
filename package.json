{
	"name": "windmill-scripts",
	"version": "3.32.2",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "module",
	"scripts": {
		"check": "bunx @biomejs/biome check --apply ./",
		"check-unsafe": "bunx @biomejs/biome check --apply-unsafe ./",
		"deps": "bunx upgradeps",
		"format": "bun run ts-format && bun run yaml-format",
		"ts-format": "bunx @biomejs/biome format --write ./",
		"yaml-format": "find ./ -type f -name '*.yaml' -exec prettier --write {} + && find ./ -type f -name '*.yml' -exec prettier --write {} +",
		"prepare": "husky",
		"precommit": "bun run format && bun run check",
		"codegen": "graphql-codegen --config ./codegen.ts"
	},
	"dependencies": {
		"@cerebruminc/neuron-sdk": "^11.18.3",
		"@faker-js/faker": "~8.4.1",
		"@linear/sdk": "~1.21.0",
		"@playwright/test": "~1.44.1",
		"@types/js-yaml": "^4.0.9",
		"app-root-path": "^3.1.0",
		"axios": "~1.6.8",
		"cheerio": "^1.0.0-rc.12",
		"crypto": "~1.0.1",
		"date-fns": "^3.6.0",
		"execa": "~8.0.1",
		"fast-xml-parser": "~4.3.2",
		"generate-password": "~1.7.1",
		"graphql-request": "~6.1.0",
		"js-yaml": "^4.1.0",
		"json-stable-stringify": "~1.0.1",
		"lodash.debounce": "~4.0.8",
		"nanoid": "~5.0.7",
		"playwright": "^1.45.3",
		"svix": "~1.24.0",
		"totp-generator": "~1.0.0",
		"typescript": "~5.4.5",
		"uuid": "~10.0.0",
		"windmill-client": "1.373.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@graphql-codegen/cli": "5.0.2",
		"@graphql-codegen/typescript": "4.0.7",
		"@graphql-codegen/typescript-graphql-request": "6.2.0",
		"@graphql-codegen/typescript-operations": "4.2.1",
		"@types/bun": "~1.1.3",
		"@types/node": "~20.12.13",
		"acorn": "~8.11.3",
		"glob": "~10.4.1",
		"husky": "^9.0.11",
		"prettier": "^3.2.5",
		"upgradeps": "^2.0.6"
	}
}
